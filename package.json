{
  "name": "@tracktak/monorepo",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "node": "npm run start:dev --prefix ./node",
    "ui": "npm run start --prefix ./ui",
    "start": "run-p node ui",
    "clean": "rimraf ./node_modules",
    "lint": "eslint ./ui/src",
    "install:ui": "cd ./ui && npm install",
    "install:node": "cd ./node && npm install",
    "install:spreadsheet": "cd ./packages/web-spreadsheet && npm install",
    "install:intrinsic-valuations": "cd ./packages/intrinsic-valuations && npm install",
    "postinstall": "run-s install:ui install:node install:spreadsheet install:intrinsic-valuations"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "editorconfig": "^0.15.3",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "lerna": "^3.22.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@tracktak/intrinsic-valuations": "file:packages\\dcf"
  }
}
